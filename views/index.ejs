    <!-- HTML forms -->

    <!-- In order that out website looks good across all platforms we need to use div -->

    <div class="container">

        <div class="container well" id="login-form">


            <form method="POST">
                <div class="form-group">
                    <label for="inputEmail">Email address</label>
                    <input name="inputEmail" type="email" class="form-control" id="inputEmail" aria-describedby="emailHelp">
                    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone
                        else.</small>
                </div>
                <div class="form-group">
                    <label for="inputPassword">Password</label>
                    <input name="inputPassword" type="password" class="form-control" id="inputPassword">
                </div>
                <div class="form-group form-check">
                    <input type="checkbox" class="form-check-input" id="exampleCheck1">
                    <label class="form-check-label" for="exampleCheck1">Check me out</label>
                </div>
                <button id="sign-in-button" formaction="/signIn" name="signInButton" type="submit" class="btn btn-primary btn-block">Sign In</button>
                <button id="create-newuser-button" formaction="/createForm" name="createNewuserButton" type="submit" class="btn btn-warning btn-block">Create New
                    Account</button>
                <button id="google-login-button" name="google-login-button" type="button" class="btn btn-primary btn-block">Sign In with
                    Google</button>

            </form>
        </div>

        <hr>


    </div>

